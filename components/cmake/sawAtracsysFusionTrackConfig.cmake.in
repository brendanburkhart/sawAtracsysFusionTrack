set (sawAtracsysFusionTrack_VERSION_MAJOR "@sawAtracsysFusionTrack_VERSION_MAJOR@")
set (sawAtracsysFusionTrack_VERSION_MINOR "@sawAtracsysFusionTrack_VERSION_MINOR@")
set (sawAtracsysFusionTrack_VERSION_PATCH "@sawAtracsysFusionTrack_VERSION_PATCH@")
set (sawAtracsysFusionTrack_VERSION "@sawAtracsysFusionTrack_VERSION@")

# figure out if this file is where it was configured (build tree) or
# not (installed tree)
set (sawAtracsysFusionTrack_CMAKE_CONFIG_FILE "@sawAtracsysFusionTrack_CMAKE_CONFIG_FILE@")
get_filename_component (THIS_FILE_PATH
                        ${CMAKE_CURRENT_LIST_FILE}
                        REALPATH)

if ("${sawAtracsysFusionTrack_CMAKE_CONFIG_FILE}" STREQUAL "${THIS_FILE_PATH}")
  # we're using the build directories
  set (sawAtracsysFusionTrack_INCLUDE_DIR "@sawAtracsysFusionTrack_INCLUDE_DIR@")
  set (sawAtracsysFusionTrack_LIBRARY_DIR "@sawAtracsysFusionTrack_LIBRARY_DIR@")
else ()
  # try to find the install dir, we know the install is using
  # share/sawAtracsysFusionTrack so we can go ../..
  set (RELATIVE_INSTALL_DIR "${THIS_FILE_PATH}/../..")
  get_filename_component (ABSOLUTE_INSTALL_DIR "${RELATIVE_INSTALL_DIR}" ABSOLUTE)
  # set directories using the install dir
  set (sawAtracsysFusionTrack_INCLUDE_DIR "${ABSOLUTE_INSTALL_DIR}/include")
  set (sawAtracsysFusionTrack_LIBRARY_DIR "${ABSOLUTE_INSTALL_DIR}/lib")
endif ()

set (sawAtracsysFusionTrack_LIBRARIES   "@sawAtracsysFusionTrack_LIBRARIES@")
